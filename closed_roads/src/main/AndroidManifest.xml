<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aware.plugin.closed_roads">

    <application
        android:label="@string/app_name">

        <service
            android:name="com.aware.plugin.closed_roads.Algorithm"
            android:exported="false" />

        <service android:name="com.aware.plugin.closed_roads.JsonParsingService"
            android:exported="false" />

        <service
            android:name="com.aware.plugin.closed_roads.Plugin"
            android:exported="false"
            android:label="@string/app_name" />

        <activity
            android:name="com.aware.plugin.closed_roads.Settings"
            android:exported="true" />

        <provider
            android:name="com.aware.plugin.closed_roads.Provider"
            android:authorities="${applicationId}.provider.closed_roads"
            android:exported="true"
            android:readPermission="com.aware.READ_CONTEXT_DATA"
            android:writePermission="com.aware.WRITE_CONTEXT_DATA" />

    </application>

</manifest>
